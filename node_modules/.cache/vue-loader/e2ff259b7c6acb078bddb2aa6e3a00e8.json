port default {\n  props: {\n    initialUser: {\n      type: Object,\n      required: true\n    }\n  },\n  data () {\n    return {\n      user: this.initialUser\n    }\n  },\n  methods: {\n\n    addFollow () {\n      this.user = {\n        ...this.user,\n        isFollowed: true\n      }\n    },\n    deleteFollowed () {\n      this.user = {\n        ...this.user,\n        isFollowed: false\n      }\n    }\n  }\n}\n</script>"]}]}